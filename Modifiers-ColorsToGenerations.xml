		<GeneralFloor>CommonFloor</GeneralFloor>
		<RedMedal>RedRing</RedMedal>
		<GoalRing>ReleaseBoxBig</GoalRing>
		<!-- Camera -->
		<ChangeVolumeCamera>EventVolumeCameraChange</ChangeVolumeCamera>
		<ObjCameraFix>FixCamera</ObjCameraFix>
		<ObjCameraPan>PanCamera</ObjCameraPan>
		<ObjCameraParallel>ParallelCamera</ObjCameraParallel>
		<ObjCameraPathPath>PathPathCamera</ObjCameraPathPath>
		<ObjCameraPathTarget>PathTargetCamera</ObjCameraPathTarget>
		<ObjCameraPoint>PointCamera</ObjCameraPoint>
		<ObjCameraNormal>Camera3D</ObjCameraNormal>
